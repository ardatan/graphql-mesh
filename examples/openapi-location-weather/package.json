{
  "name": "typescript-location-weather-example",
  "version": "0.2.13",
  "license": "MIT",
  "private": true,
  "scripts": {
    "predev": "yarn mesh:ts",
    "dev": "ts-node-dev src/index.ts",
    "prestart": "yarn mesh:ts",
    "start": "ts-node src/index.ts",
    "premesh:serve": "yarn mesh:ts",
    "mesh:serve": "graphql-mesh serve",
    "mesh:ts": "graphql-mesh typescript --output ./src/mesh/__generated__/types.ts"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/express-graphql": "0.9.0",
    "@types/node": "14.0.24",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.51",
    "typescript": "3.9.6"
  },
  "dependencies": {
    "@graphql-mesh/cli": "0.2.13",
    "@graphql-mesh/openapi": "0.2.13",
    "@graphql-mesh/runtime": "0.2.13",
    "@graphql-mesh/transform-cache": "0.2.13",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "graphql": "15.1.0"
  }
}