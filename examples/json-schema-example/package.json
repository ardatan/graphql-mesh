{
  "name": "json-schema-example",
  "version": "0.5.65",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "mesh serve",
    "prebuild": "yarn generate",
    "build": "tsc",
    "postbuild": "yarn copy-files",
    "copy-files": "cp \".meshrc.yml\" dist",
    "generate": "graphql-mesh typescript --output ./src/generated/mesh.ts && graphql-mesh generate-sdk --output ./src/generated/sdk.ts --depth 99",
    "dev": "ts-node ./index.ts",
    "start": "node ./dist"
  },
  "main": "./dist/index.js",
  "dependencies": {
    "@graphql-mesh/cli": "0.20.1",
    "@graphql-mesh/config": "^0.16.5",
    "@graphql-mesh/json-schema": "0.9.8",
    "@graphql-mesh/runtime": "^0.16.5",
    "@graphql-mesh/transform-mock": "0.6.23",
    "@graphql-mesh/transform-resolvers-composition": "^0.9.8",
    "graphql": "15.4.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.0",
    "ts-node": "10.0.0",
    "typescript": "4.2.4"
  }
}
