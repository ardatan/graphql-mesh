    "graphql": "15.0.0",
    "grpc": "1.24.2"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/google-protobuf": "3.7.2",
    "@types/node": "13.9.1",
    "concurrently": "5.1.0",
    "grpc-tools": "1.8.1",
    "grpc_tools_node_protoc_ts": "2.5.10",
    "typescript": "3.8.3"
  }
}
