{
  "name": "nextjs-apollo-example",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "mesh:generate:schema": "graphql-mesh dump-schema --dir ./server/ --output ./generated/mesh.graphql",
    "mesh:generate:json": "graphql-mesh dump-schema --dir ./server/ --output ./generated/mesh.json",
    "mesh:generate:typings": "graphql-mesh typescript --dir ./server/ --output ./generated/mesh.ts"
  },
  "dependencies": {
    "@graphql-mesh/cli": "^0.30.2",
    "@graphql-mesh/config": "^0.17.0",
    "@graphql-mesh/openapi": "^0.16.4",
    "@graphql-mesh/runtime": "^0.17.0",
    "apollo-server-micro": "^2.24.0",
    "graphql": "^15.5.0",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.4",
    "typescript": "^4.2.4"
  }
}
