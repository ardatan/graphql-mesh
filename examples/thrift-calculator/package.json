  },
  "dependencies": {
    "@creditkarma/thrift-server-core": "0.17.2",
    "@creditkarma/thrift-server-express": "1.0.4",
    "@creditkarma/thrift-typescript": "3.7.6",
    "@graphql-mesh/cli": "0.75.0",
    "@graphql-mesh/thrift": "0.9.5",
    "express": "4.18.1",
    "graphql": "16.5.0",
    "thrift": "0.16.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "concurrently": "5.3.0"
  }
}
