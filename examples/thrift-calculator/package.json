{
  "name": "thrift-calculator",
  "version": "0.5.238",
  "main": "build/main.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:thrift": "ts-node src/main.ts",
    "start:mesh": "mesh dev",
