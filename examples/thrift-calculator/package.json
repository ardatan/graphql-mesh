{
  "name": "thrift-calculator",
  "version": "0.5.326",
  "license": "MIT",
  "private": true,
  "main": "build/main.js",
  "scripts": {
    "codegen": "thrift-typescript --target thrift-server --sourceDir ./src --outDir ./src/codegen",
    "start": "concurrently \"npm run start:thrift\" \"npm run start:mesh\"",
    "start:mesh": "mesh dev",
    "start:thrift": "ts-node src/main.ts"
  },
  "dependencies": {
    "@creditkarma/thrift-server-core": "1.0.4",
    "@creditkarma/thrift-server-express": "1.0.4",
    "@creditkarma/thrift-typescript": "3.7.6",
    "@graphql-mesh/cli": "0.82.12",
    "@graphql-mesh/thrift": "0.12.5",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "thrift": "0.17.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "concurrently": "7.6.0"
  }
}
