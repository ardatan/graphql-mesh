{
  "name": "@graphql-mesh/cli",
  "version": "0.79.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/cli"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "bin": {
    "mesh": "dist/bin.js",
    "gql-mesh": "dist/bin.js",
    "graphql-mesh": "dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    }
  },
  "dependencies": {
    "ajv": "8.11.0",
    "cosmiconfig": "7.0.1",
    "@graphql-codegen/core": "2.6.4",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-generic-sdk": "3.0.4",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@graphql-codegen/typed-document-node": "2.3.6",
    "@graphql-mesh/config": "8.0.30",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@graphql-mesh/cross-helpers": "0.2.8",
    "@graphql-mesh/runtime": "0.44.30",
    "@graphql-mesh/utils": "0.42.5",
    "@graphql-mesh/store": "0.8.61",
    "@graphql-mesh/types": "0.85.6",
    "@graphql-mesh/http": "0.2.11",
    "@graphql-tools/utils": "8.13.1",
    "dotenv": "16.0.3",
    "dnscache": "1.0.2",
    "graphql-import-node": "0.0.5",
    "graphql-ws": "5.11.2",
    "change-case": "4.1.2",
    "json-bigint-patch": "0.0.8",
    "json5": "2.2.1",
    "mkdirp": "1.0.4",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "rimraf": "3.0.2",
    "yargs": "17.6.0",
    "typescript": "4.8.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "ws": "8.10.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/mkdirp": "1.0.2",
    "@types/yargs": "17.0.13",
    "@types/ws": "8.5.3",
    "@types/rimraf": "3.0.2",
    "@types/lodash.get": "4.4.7"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
