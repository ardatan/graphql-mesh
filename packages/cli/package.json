{
  "name": "@graphql-mesh/cli",
  "version": "0.54.0",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "bin": {
    "mesh": "dist/bin.js",
    "gql-mesh": "dist/bin.js",
    "graphql-mesh": "dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    }
  },
  "dependencies": {
    "ajv": "8.9.0",
    "cosmiconfig": "7.0.1",
    "@graphql-codegen/core": "2.5.1",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-jit-sdk": "2.0.1",
    "@graphql-codegen/typescript-operations": "2.3.0",
    "@graphql-mesh/config": "0.29.1",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "@graphql-mesh/runtime": "0.30.0",
    "@graphql-mesh/utils": "0.27.0",
    "@graphql-mesh/store": "0.3.13",
    "@graphql-tools/code-file-loader": "7.2.3",
    "@graphql-tools/utils": "8.6.1",
    "@graphql-tools/load": "7.5.1",
    "@graphql-tools/graphql-file-loader": "7.3.3",
    "@graphql-tools/wrap": "8.3.3",
    "body-parser": "1.19.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "14.3.2",
    "graphql-import-node": "0.0.4",
    "express": "4.17.2",
    "graphql-helix": "1.11.0",
    "graphql-upload": "13.0.0",
    "graphql-ws": "5.5.5",
    "change-case": "4.1.2",
    "json-bigint-patch": "0.0.4",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "rimraf": "3.0.2",
    "yargs": "17.3.1",
    "typescript": "4.5.5",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "ws": "8.4.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/graphql-upload": "8.0.10",
    "@types/mkdirp": "1.0.2",
    "@types/yargs": "17.0.8",
    "@types/ws": "8.2.2",
    "@types/rimraf": "3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
