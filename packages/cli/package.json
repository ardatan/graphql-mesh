{
  "name": "@graphql-mesh/cli",
  "version": "0.42.6",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "bin": {
    "mesh": "dist/bin.js",
    "gql-mesh": "dist/bin.js",
    "graphql-mesh": "dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    }
  },
  "dependencies": {
    "ajv": "8.6.3",
    "cosmiconfig": "7.0.1",
    "@graphql-codegen/core": "2.2.0",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-generic-sdk": "2.1.6",
    "@graphql-codegen/typescript-operations": "2.1.8",
    "@graphql-mesh/config": "0.24.4",
    "@graphql-codegen/typescript-resolvers": "2.3.2",
    "@graphql-mesh/runtime": "0.24.1",
    "@graphql-mesh/utils": "0.19.0",
    "@graphql-mesh/store": "0.1.19",
    "@graphql-tools/code-file-loader": "7.2.1",
    "@graphql-tools/utils": "8.5.0",
    "@graphql-tools/load": "7.4.0",
    "@graphql-tools/graphql-file-loader": "7.3.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql-helix": "1.8.4",
    "graphql-upload": "12.0.0",
    "graphql-ws": "5.5.3",
    "change-case": "4.1.2",
    "js-yaml": "4.1.0",
    "json-bigint-patch": "0.0.4",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "rimraf": "3.0.2",
    "yargs": "17.2.1",
    "typescript": "4.4.4",
    "ws": "8.2.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/graphql-upload": "8.0.7",
    "@types/js-yaml": "4.0.4",
    "@types/mkdirp": "1.0.2",
    "@types/yargs": "17.0.4",
    "@types/ws": "8.2.0",
    "@types/rimraf": "3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
