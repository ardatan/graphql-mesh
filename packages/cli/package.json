{
  "name": "@graphql-mesh/cli",
  "version": "0.82.30",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "bin": {
    "mesh": "dist/cjs/bin.js",
    "gql-mesh": "dist/cjs/bin.js",
    "graphql-mesh": "dist/cjs/bin.js",
    "graphql-mesh-esm": "dist/esm/bin.js"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-codegen/core": "3.1.0",
    "@graphql-codegen/typed-document-node": "3.0.2",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-generic-sdk": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@graphql-mesh/config": "10.1.13",
    "@graphql-mesh/cross-helpers": "0.3.4",
    "@graphql-mesh/http": "0.3.26",
    "@graphql-mesh/runtime": "0.46.21",
    "@graphql-mesh/store": "0.9.20",
    "@graphql-mesh/types": "0.91.12",
    "@graphql-mesh/utils": "0.43.20",
    "@graphql-tools/utils": "9.2.1",
    "ajv": "8.12.0",
    "change-case": "4.1.2",
    "cosmiconfig": "8.1.3",
    "dnscache": "1.0.2",
    "dotenv": "16.0.3",
    "graphql-import-node": "0.0.5",
    "graphql-ws": "5.12.0",
    "json-bigint-patch": "0.0.8",
    "json5": "2.2.3",
    "mkdirp": "2.1.6",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "rimraf": "4.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "tslib": "^2.4.0",
    "typescript": "5.0.3",
    "ws": "8.13.0",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "@types/lodash.get": "4.4.7",
    "@types/mkdirp": "1.0.2",
    "@types/rimraf": "3.0.2",
    "@types/ws": "8.5.4",
    "@types/yargs": "17.0.24"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
