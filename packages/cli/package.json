  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    }
  },
  "dependencies": {
    "@graphql-codegen/core": "1.13.2",
    "@graphql-codegen/typescript": "1.13.2",
    "@graphql-codegen/typescript-generic-sdk": "1.13.2",
    "@graphql-codegen/typescript-operations": "1.13.2",
    "@graphql-codegen/typescript-resolvers": "1.13.2",
    "@graphql-mesh/runtime": "0.0.21",
    "@graphql-toolkit/code-file-loader": "0.10.3",
    "@graphql-toolkit/common": "0.10.3",
    "@graphql-toolkit/core": "0.10.3",
    "@graphql-toolkit/graphql-file-loader": "0.10.3",
    "apollo-server-express": "2.12.0",
    "chalk": "4.0.0",
    "change-case": "4.1.1",
    "express": "4.17.1",
    "graphql-playground-html": "1.6.19",
    "js-yaml": "3.13.1",
    "mkdirp": "1.0.4",
    "pascal-case": "3.1.1",
    "winston": "3.2.1",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@types/js-yaml": "3.12.3",
    "@types/mkdirp": "1.0.0",
    "@types/yargs": "15.0.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
