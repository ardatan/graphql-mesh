{
  "name": "@graphql-mesh/cli",
  "version": "0.30.2",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "bin": {
    "mesh": "dist/bin.js",
    "gql-mesh": "dist/bin.js",
    "graphql-mesh": "dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    }
  },
  "dependencies": {
    "@graphql-codegen/core": "1.17.10",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-generic-sdk": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@graphql-mesh/config": "0.17.0",
    "@graphql-codegen/typescript-resolvers": "1.19.2",
    "@graphql-mesh/runtime": "0.17.0",
    "@graphql-mesh/utils": "0.12.0",
    "@graphql-tools/code-file-loader": "6.3.1",
    "@graphql-tools/utils": "7.10.0",
    "@graphql-tools/load": "6.2.8",
    "@graphql-tools/graphql-file-loader": "6.2.7",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql-helix": "1.6.1",
    "graphql-upload": "12.0.0",
    "graphql-ws": "5.1.2",
    "ora": "5.4.1",
    "chalk": "4.1.1",
    "change-case": "4.1.2",
    "js-yaml": "4.1.0",
    "json-bigint-patch": "0.0.4",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "winston": "3.3.3",
    "yargs": "17.0.1",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.12",
    "@types/graphql-upload": "8.0.5",
    "@types/js-yaml": "4.0.1",
    "@types/mkdirp": "1.0.1",
    "@types/yargs": "17.0.0",
    "@types/ws": "7.4.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
