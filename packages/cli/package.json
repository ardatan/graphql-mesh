{
  "name": "@graphql-mesh/cli",
  "version": "0.0.21",
  "license": "MIT",
  "scripts": {
    "prepack": "bob prepack"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "bin": {
    "mesh": "dist/bin.js",
    "gql-mesh": "dist/bin.js",
    "graphql-mesh": "dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
