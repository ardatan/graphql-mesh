{
  "name": "@graphql-mesh/cli",
  "version": "0.82.9",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "bin": {
    "mesh": "dist/cjs/bin.js",
    "gql-mesh": "dist/cjs/bin.js",
    "graphql-mesh": "dist/cjs/bin.js",
    "graphql-mesh-esm": "dist/esm/bin.js"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-codegen/core": "2.6.8",
    "@graphql-codegen/typed-document-node": "2.3.12",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-generic-sdk": "3.0.4",
    "@graphql-codegen/typescript-operations": "2.5.11",
    "@graphql-codegen/typescript-resolvers": "2.7.11",
    "@graphql-mesh/config": "10.0.5",
    "@graphql-mesh/cross-helpers": "0.3.0",
    "@graphql-mesh/http": "0.3.7",
    "@graphql-mesh/runtime": "0.46.4",
    "@graphql-mesh/store": "0.9.4",
    "@graphql-mesh/types": "0.89.3",
    "@graphql-mesh/utils": "0.43.4",
    "@graphql-tools/utils": "9.1.3",
    "ajv": "8.12.0",
    "change-case": "4.1.2",
    "cosmiconfig": "8.0.0",
    "dnscache": "1.0.2",
    "dotenv": "16.0.3",
    "graphql-import-node": "0.0.5",
    "graphql-ws": "5.11.2",
    "json-bigint-patch": "0.0.8",
    "json5": "2.2.3",
    "mkdirp": "1.0.4",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "tslib": "^2.4.0",
    "typescript": "4.9.4",
    "ws": "8.11.0",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/lodash.get": "4.4.7",
    "@types/mkdirp": "1.0.2",
    "@types/rimraf": "3.0.2",
    "@types/ws": "8.5.4",
    "@types/yargs": "17.0.19"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
