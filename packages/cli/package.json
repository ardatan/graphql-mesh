{
  "name": "@graphql-mesh/cli",
  "version": "0.68.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/cli"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "bin": {
    "mesh": "dist/bin.js",
    "gql-mesh": "dist/bin.js",
    "graphql-mesh": "dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "bin": {
      "graphql-mesh": {
        "input": "src/bin.ts"
      }
    }
  },
  "dependencies": {
    "ajv": "8.11.0",
    "cosmiconfig": "7.0.1",
    "@graphql-codegen/core": "2.5.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-generic-sdk": "2.3.7",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typed-document-node": "2.2.8",
    "@graphql-mesh/config": "0.35.2",
    "@graphql-codegen/typescript-resolvers": "2.6.1",
    "@graphql-mesh/cross-helpers": "0.1.2",
    "@graphql-mesh/runtime": "0.34.2",
    "@graphql-mesh/utils": "0.34.2",
    "@graphql-mesh/store": "0.8.0",
    "@graphql-mesh/types": "0.71.2",
    "@graphql-tools/code-file-loader": "7.2.14",
    "@graphql-tools/utils": "8.6.9",
    "@graphql-tools/load": "7.5.10",
    "@graphql-tools/graphql-file-loader": "7.3.11",
    "@graphql-tools/wrap": "8.4.16",
    "@graphql-yoga/node": "2.3.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "dnscache": "1.0.2",
    "graphql-import-node": "0.0.4",
    "express": "4.18.0",
    "graphql-ws": "5.8.1",
    "change-case": "4.1.2",
    "json-bigint-patch": "0.0.8",
    "lodash": "4.17.21",
    "mkdirp": "1.0.4",
    "open": "7.4.2",
    "pascal-case": "3.1.2",
    "rimraf": "3.0.2",
    "yargs": "17.4.1",
    "typescript": "4.6.3",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "ws": "8.5.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/mkdirp": "1.0.2",
    "@types/yargs": "17.0.10",
    "@types/ws": "8.5.3",
    "@types/rimraf": "3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
