{
  "name": "@graphql-mesh/cache-localforage",
  "version": "0.6.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/cache/localforage"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-mesh/types": "0.73.3",
    "@graphql-mesh/utils": "0.35.3",
    "localforage": "1.10.0",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
