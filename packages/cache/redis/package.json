  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/cache/redis"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
