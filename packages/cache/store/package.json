{
  "name": "@graphql-mesh/cache-store",
  "version": "0.1.31",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.57.1",
    "@graphql-mesh/store": "0.3.7"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
