{
  "name": "@graphql-mesh/transform-federation",
  "version": "0.98.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/legacy/transforms/federation"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/types": "^0.98.8",
    "@graphql-mesh/utils": "^0.98.8",
    "@graphql-tools/utils": "^10.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@apollo/subgraph": "^2.4.1",
    "@graphql-mesh/string-interpolation": "^0.5.4",
    "@graphql-tools/delegate": "^10.0.11",
    "@graphql-tools/stitching-directives": "^3.0.0",
    "dset": "^3.1.2",
    "graphql-transform-federation": "^2.2.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
