{
  "name": "@graphql-mesh/grpc",
  "version": "0.99.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/legacy/handlers/grpc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
