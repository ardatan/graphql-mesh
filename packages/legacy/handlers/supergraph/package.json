{
  "name": "@graphql-mesh/supergraph",
  "version": "0.10.21",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/legacy/handlers/supergraph"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.11",
    "@graphql-mesh/store": "^0.104.21",
    "@graphql-mesh/string-interpolation": "^0.5.10",
    "@graphql-mesh/types": "^0.104.18",
    "@graphql-mesh/utils": "^0.104.19",
    "@graphql-tools/federation": "^4.2.7",
    "@graphql-tools/url-loader": "^9.0.6",
    "@graphql-tools/utils": "^11.0.0",
    "lodash.get": "^4.4.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "graphql": "16.12.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
