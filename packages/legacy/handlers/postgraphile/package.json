{
  "name": "@graphql-mesh/postgraphile",
  "version": "0.104.27",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/legacy/handlers/postgraphile"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.12",
    "@graphql-mesh/store": "^0.104.26",
    "@graphql-mesh/string-interpolation": "^0.5.11",
    "@graphql-mesh/types": "^0.104.22",
    "@graphql-mesh/utils": "^0.104.24",
    "@graphql-tools/delegate": "^12.0.8",
    "pg": "^8.19.0",
    "postgraphile": "^4.13.0",
    "postgraphile-core": "^4.13.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "graphql": "16.13.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
