{
  "name": "@graphql-mesh/json-schema",
  "version": "0.108.24",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/legacy/handlers/json-schema"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.10",
    "@graphql-mesh/store": "^0.103.21",
    "@graphql-mesh/string-interpolation": "^0.5.8",
    "@graphql-mesh/types": "^0.103.21",
    "@graphql-mesh/utils": "^0.103.21",
    "@json-schema-tools/meta-schema": "^1.7.5",
    "@omnigraph/json-schema": "^0.108.23",
    "graphql-compose": "^9.1.0",
    "json-machete": "^0.97.6",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "graphql": "16.10.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
