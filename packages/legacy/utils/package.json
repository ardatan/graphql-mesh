    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "^0.5.4",
    "@graphql-tools/delegate": "^10.0.11",
    "@whatwg-node/fetch": "^0.9.13",
    "disposablestack": "^1.1.6",
    "dset": "^3.1.2",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.topath": "^4.5.2",
    "tiny-lru": "^11.0.0"
  },
  "devDependencies": {
