{
  "name": "@graphql-mesh/runtime",
  "version": "0.106.20",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/legacy/runtime"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@envelop/core": "^5.3.2",
    "@envelop/extended-validation": "^7.0.0",
    "@envelop/graphql-jit": "^11.0.0",
    "@graphql-mesh/cross-helpers": "^0.4.11",
    "@graphql-mesh/string-interpolation": "^0.5.10",
    "@graphql-mesh/types": "^0.104.18",
    "@graphql-mesh/utils": "^0.104.19",
    "@graphql-tools/batch-delegate": "^10.0.10",
    "@graphql-tools/delegate": "^12.0.4",
    "@graphql-tools/executor": "^1.5.1",
    "@graphql-tools/utils": "^11.0.0",
    "@graphql-tools/wrap": "^11.1.4",
    "@whatwg-node/fetch": "^0.10.6",
    "@whatwg-node/promise-helpers": "^1.0.0",
    "graphql-jit": "^0.8.7",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-tools/schema": "10.0.31",
    "graphql": "16.12.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
