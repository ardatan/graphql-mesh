{
  "name": "json-machete",
  "version": "0.2.1",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@json-schema-tools/meta-schema": "1.6.19",
    "cross-undici-fetch": "0.1.4",
    "json-ptr": "3.0.1",
    "@graphql-mesh/utils": "0.22.2",
    "url-join": "4.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
