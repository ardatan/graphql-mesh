{
  "name": "@graphql-mesh/transport-neo4j",
  "version": "0.4.7",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/transports/neo4j"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/transport-common": "^0.4.7",
    "@graphql-mesh/types": "^0.99.7",
    "@graphql-mesh/utils": "^0.99.7",
    "@graphql-tools/delegate": "^10.0.18",
    "@graphql-tools/utils": "^10.3.4",
    "@neo4j/graphql": "^5.0.0",
    "graphql-scalars": "^1.22.4",
    "neo4j-driver": "^5.17.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
