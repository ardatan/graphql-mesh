{
  "name": "@graphql-mesh/transport-neo4j",
  "version": "0.10.17",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/transports/neo4j"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.11",
    "@graphql-mesh/transport-common": "^1.0.13",
    "@graphql-mesh/types": "^0.104.18",
    "@graphql-mesh/utils": "^0.104.19",
    "@graphql-tools/delegate": "^12.0.4",
    "@graphql-tools/utils": "^11.0.0",
    "@neo4j/graphql": "^7.3.3",
    "graphql-scalars": "^1.22.4",
    "neo4j-driver": "^6.0.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "graphql": "16.12.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
