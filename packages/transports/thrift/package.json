{
  "name": "@graphql-mesh/transport-thrift",
  "version": "0.2.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/transports/thrift"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@creditkarma/thrift-client": "^1.0.4",
    "@creditkarma/thrift-server-core": "^1.0.4",
    "@graphql-mesh/string-interpolation": "^0.5.4",
    "@graphql-mesh/transport-common": "^0.2.8",
    "@graphql-mesh/utils": "^0.98.8",
    "@graphql-tools/utils": "^10.2.1",
    "pascal-case": "^3.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
