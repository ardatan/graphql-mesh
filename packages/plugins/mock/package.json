{
  "name": "@graphql-mesh/plugin-mock",
  "version": "0.1.22",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/mock"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/types": "^0.91.13",
    "@graphql-mesh/utils": "^0.43.21",
    "@graphql-tools/utils": "^9.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.4.4",
    "@graphql-tools/mock": "8.7.19",
    "@graphql-tools/schema": "9.0.17",
    "faker": "5.5.3",
    "graphql-scalars": "^1.20.4"
  },
  "devDependencies": {
    "@envelop/core": "3.0.6",
    "@graphql-mesh/cache-localforage": "0.7.20",
    "@types/faker": "5.5.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
