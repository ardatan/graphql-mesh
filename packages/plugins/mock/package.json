{
  "name": "@graphql-mesh/plugin-mock",
  "version": "0.0.37",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/mock"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.9",
    "@graphql-mesh/types": "0.85.7",
    "@graphql-mesh/utils": "0.42.6",
    "@graphql-mesh/string-interpolation": "0.3.3",
    "@graphql-tools/utils": "9.1.0",
    "@graphql-tools/mock": "8.7.11",
    "@graphql-tools/schema": "9.0.9",
    "faker": "5.5.3",
    "graphql-scalars": "1.20.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/faker": "5.5.9",
    "@envelop/core": "3.0.3",
    "@graphql-mesh/cache-localforage": "0.6.60"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
