{
  "name": "@graphql-mesh/plugin-mock",
  "version": "0.98.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/plugins/mock"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.3",
    "@graphql-mesh/types": "^0.98.8",
    "@graphql-mesh/utils": "^0.98.8",
    "@graphql-tools/utils": "^10.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "^0.5.4",
    "@graphql-tools/mock": "^9.0.3",
    "@graphql-tools/schema": "^10.0.0",
    "faker": "5.5.3",
    "graphql-scalars": "^1.22.4"
  },
  "devDependencies": {
    "@envelop/core": "5.0.1",
    "@graphql-mesh/cache-localforage": "0.98.8",
    "@types/faker": "5.5.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
