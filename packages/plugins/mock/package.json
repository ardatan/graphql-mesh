{
  "name": "@graphql-mesh/plugin-mock",
  "version": "0.0.40",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/mock"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.10",
    "@graphql-mesh/string-interpolation": "0.3.3",
    "@graphql-mesh/types": "0.87.1",
    "@graphql-mesh/utils": "0.42.9",
    "@graphql-tools/mock": "8.7.14",
    "@graphql-tools/schema": "9.0.12",
    "@graphql-tools/utils": "9.1.3",
    "faker": "5.5.3",
    "graphql-scalars": "1.20.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@envelop/core": "3.0.4",
    "@graphql-mesh/cache-localforage": "0.6.63",
    "@types/faker": "5.5.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
