{
  "name": "@graphql-mesh/plugin-http-cache",
  "version": "0.0.3",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/http-cache"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.85.2",
    "@graphql-mesh/utils": "0.42.1",
    "http-cache-semantics": "4.1.0",
    "@whatwg-node/fetch": "0.5.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/http-cache-semantics": "4.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
