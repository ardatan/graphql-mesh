{
  "name": "@graphql-mesh/plugin-newrelic",
  "version": "0.2.28",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/newrelic"
  },
  "peerDependencies": {
    "graphql": "*",
    "newrelic": "^7 || ^8.0.0"
  },
  "dependencies": {
    "@envelop/newrelic": "5.0.3",
    "@graphql-mesh/types": "0.85.7",
    "@graphql-mesh/utils": "0.42.6",
    "@graphql-mesh/cross-helpers": "0.2.9",
    "@graphql-mesh/string-interpolation": "0.3.3",
    "@envelop/core": "^3.0.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/newrelic": "7.0.4",
    "newrelic": "8.17.1"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
