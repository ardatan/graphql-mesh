{
  "name": "@graphql-mesh/plugin-newrelic",
  "version": "0.3.22",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/newrelic"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/types": "^0.91.13",
    "@graphql-mesh/utils": "^0.43.21",
    "graphql": "*",
    "newrelic": "^7 || ^8.0.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@envelop/core": "^3.0.0",
    "@envelop/newrelic": "5.0.6",
    "@graphql-mesh/string-interpolation": "0.4.4"
  },
  "devDependencies": {
    "@types/newrelic": "7.0.4",
    "newrelic": "8.17.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
