{
  "name": "@graphql-mesh/plugin-newrelic",
  "version": "0.98.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/plugins/newrelic"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.3",
    "@graphql-mesh/types": "^0.98.8",
    "@graphql-mesh/utils": "^0.98.8",
    "graphql": "*",
    "graphql-yoga": "^4.0.5 || ^5.0.0",
    "newrelic": "^7 || ^8.0.0 || ^10.0.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@envelop/core": "^5.0.0",
    "@envelop/newrelic": "^7.0.0",
    "@graphql-mesh/string-interpolation": "^0.5.4"
  },
  "devDependencies": {
    "@newrelic/test-utilities": "6.5.5",
    "@types/newrelic": "9.14.4",
    "graphql-yoga": "5.4.0",
    "newrelic": "10.6.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
