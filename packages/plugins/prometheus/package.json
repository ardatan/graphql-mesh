{
  "name": "@graphql-mesh/plugin-prometheus",
  "version": "0.100.6",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/plugins/prometheus"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/types": "^0.98.8",
    "@graphql-mesh/utils": "^0.98.8",
    "graphql": "*",
    "graphql-yoga": "^4.0.5 || ^5.0.0",
    "prom-client": "^13 || ^14.0.0 || ^15.0.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/serve-runtime": "^0.3.11",
    "@graphql-yoga/plugin-prometheus": "^5.0.0"
  },
  "devDependencies": {
    "prom-client": "15.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
