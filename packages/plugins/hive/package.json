{
  "name": "@graphql-mesh/plugin-hive",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/plugins/hive"
  },
  "license": "MIT",
  "main": "dist/index.js",
