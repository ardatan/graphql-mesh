  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/types": "^0.91.13",
    "@graphql-mesh/utils": "^0.43.21",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/runtime": "0.46.22",
    "@whatwg-node/router": "0.3.0",
    "graphql-yoga": "3.8.0"
  },
  "devDependencies": {
    "@types/lodash.get": "4.4.7",
    "@types/mkdirp": "1.0.2",
    "@types/rimraf": "3.0.2",
    "@types/ws": "8.5.4",
    "@types/yargs": "17.0.24"
  },
  "publishConfig": {
