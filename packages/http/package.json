{
  "name": "@graphql-mesh/http",
  "version": "0.2.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/http"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@whatwg-node/server": "^0.4.10",
    "@whatwg-node/fetch": "^0.5.0",
    "@graphql-mesh/cross-helpers": "0.2.9",
    "@graphql-mesh/runtime": "0.44.31",
    "@graphql-mesh/utils": "0.42.6",
    "@graphql-mesh/types": "0.85.7",
    "graphql-yoga": "3.0.0-next.12",
    "itty-router": "2.6.6",
    "itty-router-extras": "0.4.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/itty-router-extras": "0.4.0",
    "@types/mkdirp": "1.0.2",
    "@types/yargs": "17.0.13",
    "@types/ws": "8.5.3",
    "@types/rimraf": "3.0.2",
    "@types/lodash.get": "4.4.7"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
