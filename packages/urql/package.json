{
  "name": "@graphql-mesh/urql-exchange",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@urql/core": "2.4.3",
    "wonka": "4.0.15"
  },
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.32.0",
    "graphql": "^15.2.0 || ^16.0.0",
    "@urql/core": "^2.4.3",
    "wonka": "^4.0.15"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
