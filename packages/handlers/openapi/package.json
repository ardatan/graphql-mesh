{
  "name": "@graphql-mesh/openapi",
  "version": "0.28.4",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/openapi"
  },
  "devDependencies": {
    "@types/deep-equal": "1.0.1",
    "@types/form-urlencoded": "2.0.1",
    "@types/json-pointer": "1.0.31"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.0",
    "@graphql-mesh/types": "0.78.3",
    "@graphql-mesh/store": "0.8.23",
    "@graphql-mesh/string-interpolation": "0.3.0",
    "@graphql-mesh/utils": "0.37.4",
    "@graphql-tools/utils": "8.8.0",
    "@whatwg-node/fetch": "^0.0.2",
    "deep-equal": "2.0.5",
    "form-urlencoded": "6.0.7",
    "jsonpath-plus": "7.0.0",
    "json-pointer": "0.6.2",
    "openapi-diff": "0.23.5",
    "graphql-scalars": "1.17.0",
    "pluralize": "8.0.0",
    "swagger2openapi": "7.0.8",
    "url-join": "4.0.1",
    "openapi-types": "12.0.0",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "buildOptions": {
    "external": [
      "cross-fetch"
    ]
  }
}
