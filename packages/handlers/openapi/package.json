{
  "name": "@graphql-mesh/openapi",
  "version": "0.18.17",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/deep-equal": "1.0.1",
    "@types/form-urlencoded": "2.0.1"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.50.0",
    "@graphql-mesh/store": "0.1.15",
    "@graphql-mesh/utils": "0.17.1",
    "@graphql-tools/utils": "8.2.5",
    "deep-equal": "2.0.5",
    "form-urlencoded": "6.0.4",
    "jsonpath-plus": "6.0.1",
    "json-ptr": "2.2.0",
    "openapi-diff": "0.23.4",
    "graphql-scalars": "1.11.0",
    "graphql-subscriptions": "1.2.1",
    "pluralize": "8.0.0",
    "swagger2openapi": "7.0.8",
    "url-join": "4.0.1",
    "openapi-types": "9.3.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "buildOptions": {
    "external": [
      "cross-fetch"
    ]
  }
}
