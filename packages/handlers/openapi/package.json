{
  "name": "@graphql-mesh/openapi",
  "version": "0.16.11",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/deep-equal": "1.0.1",
    "@types/form-urlencoded": "2.0.1"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.42.0",
    "@graphql-mesh/utils": "0.12.0",
    "deep-equal": "2.0.5",
    "form-urlencoded": "6.0.4",
    "jsonpath-plus": "5.0.7",
    "json-ptr": "2.2.0",
    "graphql-scalars": "1.10.0",
    "graphql-subscriptions": "1.2.1",
    "pluralize": "8.0.0",
    "swagger2openapi": "7.0.7",
    "url-join": "4.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "buildOptions": {
    "external": [
      "cross-fetch"
    ]
  }
}
