{
  "name": "@graphql-mesh/odata",
  "version": "0.17.27",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/odata"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/http-string-parser": "0.0.30",
    "@types/url-join": "4.0.1"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.1.6",
    "@graphql-mesh/types": "0.74.1",
    "@graphql-mesh/store": "0.8.14",
    "@graphql-mesh/string-interpolation": "0.3.0",
    "@graphql-mesh/utils": "0.35.5",
    "@graphql-tools/utils": "8.6.13",
    "http-string-parser": "0.0.6",
    "cross-undici-fetch": "^0.4.0",
    "dataloader": "2.1.0",
    "fast-xml-parser": "4.0.8",
    "graphql-compose": "9.0.8",
    "graphql-parse-resolve-info": "4.12.0",
    "graphql-scalars": "1.17.0",
    "pascal-case": "3.1.2",
    "url-join": "4.0.1",
    "tslib": "^2.4.0",
    "@graphql-tools/delegate": "8.7.11"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
