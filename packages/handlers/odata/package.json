{
  "name": "@graphql-mesh/odata",
  "version": "0.13.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/http-string-parser": "0.0.29",
    "@types/url-join": "4.0.1"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.51.0",
    "@graphql-mesh/store": "0.1.16",
    "@graphql-mesh/utils": "0.17.2",
    "@graphql-tools/utils": "8.2.5",
    "http-string-parser": "0.0.6",
    "cross-fetch": "3.1.4",
    "dataloader": "2.0.0",
    "fast-xml-parser": "3.20.3",
    "graphql-compose": "9.0.3",
    "graphql-parse-resolve-info": "4.12.0",
    "graphql-scalars": "1.11.1",
    "pascal-case": "3.1.2",
    "url-join": "4.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
