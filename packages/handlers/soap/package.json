{
  "name": "@graphql-mesh/soap",
  "version": "0.7.10",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/soap": "0.21.0"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.42.0",
    "@graphql-mesh/utils": "0.12.0",
    "soap-graphql": "0.1.1",
    "soap": "0.39.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
