{
  "name": "@graphql-mesh/graphql",
  "version": "0.25.2",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/graphql"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.8.4",
    "@graphql-mesh/utils": "0.34.6",
    "@graphql-mesh/types": "0.72.1",
    "@graphql-tools/url-loader": "7.9.20",
    "@graphql-tools/wrap": "8.4.16",
    "@graphql-tools/delegate": "8.7.7",
    "@graphql-tools/utils": "8.6.9",
    "lodash": "4.17.21",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
