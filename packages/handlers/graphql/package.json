{
  "name": "@graphql-mesh/graphql",
  "version": "0.31.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/graphql"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.8.33",
    "@graphql-mesh/cross-helpers": "0.2.3",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "@graphql-mesh/utils": "0.41.0",
    "@graphql-mesh/types": "0.81.0",
    "@graphql-tools/url-loader": "7.14.1",
    "@graphql-tools/wrap": "9.0.5",
    "@graphql-tools/delegate": "9.0.4",
    "@graphql-tools/utils": "8.10.1",
    "lodash.get": "4.4.2",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
