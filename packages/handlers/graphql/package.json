{
  "name": "@graphql-mesh/graphql",
  "version": "0.27.3",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/graphql"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.8.16",
    "@graphql-mesh/cross-helpers": "0.1.6",
    "@graphql-mesh/string-interpolation": "0.3.0",
    "@graphql-mesh/utils": "0.35.7",
    "@graphql-mesh/types": "0.75.0",
    "@graphql-tools/url-loader": "7.9.25",
    "@graphql-tools/wrap": "8.4.20",
    "@graphql-tools/delegate": "8.7.11",
    "@graphql-tools/utils": "8.6.13",
    "lodash.get": "4.4.2",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
