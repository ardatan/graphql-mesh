{
  "name": "@graphql-mesh/graphql",
  "version": "0.21.1",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.3.24",
    "@graphql-mesh/utils": "0.28.1",
    "@graphql-mesh/types": "0.66.1",
    "@graphql-tools/url-loader": "7.8.0",
    "@graphql-tools/wrap": "8.4.3",
    "@graphql-tools/delegate": "8.5.2",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
