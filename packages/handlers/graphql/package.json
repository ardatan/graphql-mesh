{
  "name": "@graphql-mesh/graphql",
  "version": "0.18.18",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.1.18",
    "@graphql-mesh/utils": "0.18.1",
    "@graphql-mesh/types": "0.53.0",
    "@graphql-tools/url-loader": "7.2.1",
    "@graphql-tools/wrap": "8.2.0",
    "@graphql-tools/delegate": "8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
