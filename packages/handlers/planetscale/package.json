{
  "name": "@graphql-mesh/planetscale",
  "version": "0.19.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/planetscale"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.3.1",
    "@graphql-mesh/store": "0.9.6",
    "@graphql-mesh/string-interpolation": "0.4.2",
    "@graphql-mesh/types": "0.89.5",
    "@graphql-mesh/utils": "0.43.6",
    "@graphql-tools/delegate": "9.0.24",
    "@graphql-tools/utils": "9.1.4",
    "@planetscale/database": "^1.5.0",
    "graphql-compose": "9.0.10",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "1.20.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/graphql-fields": "1.3.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.js"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
