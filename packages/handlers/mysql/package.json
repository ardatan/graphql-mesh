{
  "name": "@graphql-mesh/mysql",
  "version": "0.15.5",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/mysql"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.70.1",
    "@graphql-mesh/store": "0.7.1",
    "@graphql-mesh/utils": "0.33.1",
    "graphql-compose": "9.0.8",
    "graphql-fields": "2.0.3",
    "mysql": "2.18.1",
    "mysql-utilities": "1.1.3",
    "graphql-scalars": "1.17.0",
    "tslib": "^2.3.1",
    "@graphql-tools/utils": "8.6.5"
  },
  "devDependencies": {
    "@types/graphql-fields": "1.3.4",
    "@types/mysql": "2.15.21"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
