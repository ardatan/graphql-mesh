{
  "name": "@graphql-mesh/mysql",
  "version": "0.19.21",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/mysql"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/store": "^0.9.20",
    "@graphql-mesh/types": "^0.91.13",
    "@graphql-mesh/utils": "^0.43.21",
    "@graphql-tools/utils": "^9.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.4.4",
    "@graphql-tools/delegate": "9.0.28",
    "graphql-compose": "9.0.10",
    "graphql-fields": "2.0.3",
    "graphql-scalars": "^1.20.4",
    "mysql": "2.18.1",
    "mysql-utilities": "1.1.4"
  },
  "devDependencies": {
    "@types/graphql-fields": "1.3.5",
    "@types/mysql": "2.15.21"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.js"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
