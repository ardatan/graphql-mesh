{
  "name": "@graphql-mesh/postgraphile",
  "version": "0.20.5",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/postgraphile"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.1.0",
    "@graphql-mesh/types": "0.70.1",
    "@graphql-mesh/store": "0.7.1",
    "@graphql-mesh/utils": "0.33.1",
    "postgraphile-core": "4.12.2",
    "postgraphile": "4.12.9",
    "pg": "^8.6.0",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
