{
  "name": "@graphql-mesh/postgraphile",
  "version": "0.22.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/postgraphile"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.3.0",
    "@graphql-mesh/store": "0.9.3",
    "@graphql-mesh/string-interpolation": "0.4.1",
    "@graphql-mesh/types": "0.89.2",
    "@graphql-mesh/utils": "0.43.3",
    "@graphql-tools/delegate": "9.0.21",
    "pg": "^8.6.0",
    "postgraphile": "4.12.12",
    "postgraphile-core": "4.12.3",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
