{
  "name": "@graphql-mesh/postgraphile",
  "version": "0.18.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.57.2",
    "@graphql-mesh/store": "0.3.8",
    "@graphql-mesh/utils": "0.26.0",
    "postgraphile-core": "4.12.2",
    "postgraphile": "4.12.8",
    "pg": "^8.6.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
