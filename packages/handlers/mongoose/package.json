{
  "name": "@graphql-mesh/mongoose",
  "version": "0.11.31",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/mongoose"
  },
  "license": "MIT",
  "main": "dist/index.js",
