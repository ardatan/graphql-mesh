{
  "name": "@graphql-mesh/grpc",
  "version": "0.14.9",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.42.0",
    "@graphql-mesh/utils": "0.12.0",
    "@grpc/grpc-js": "^1.1.7",
    "@grpc/proto-loader": "0.6.2",
    "camel-case": "4.1.2",
    "graphql-scalars": "1.9.3",
    "graphql-compose": "8.1.0",
    "grpc-reflection-js": "0.1.1",
    "protobufjs": "6.11.2",
    "pascal-case": "3.1.2",
    "lodash": "4.17.21"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
