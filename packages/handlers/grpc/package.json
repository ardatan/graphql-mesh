      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/grpc"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.1.3",
    "@graphql-mesh/types": "0.72.2",
    "@graphql-mesh/utils": "0.34.7",
    "@graphql-mesh/store": "0.8.5",
    "@grpc/grpc-js": "^1.1.7",
    "@grpc/proto-loader": "0.6.12",
    "graphql-scalars": "1.17.0",
    "graphql-compose": "9.0.8",
    "grpc-reflection-js": "0.1.2",
    "globby": "11.1.0",
    "long": "4.0.0",
    "protobufjs": "6.11.2",
    "lodash": "4.17.21",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
