{
  "name": "@graphql-mesh/grpc",
  "version": "0.18.1",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.55.0",
    "@graphql-mesh/utils": "0.21.1",
    "@graphql-mesh/store": "0.2.3",
    "@grpc/grpc-js": "^1.1.7",
    "@grpc/proto-loader": "0.6.7",
    "graphql-scalars": "1.13.6",
    "graphql-compose": "9.0.4",
    "grpc-reflection-js": "0.1.2",
    "globby": "11.0.4",
    "long": "4.0.0",
    "protobufjs": "6.11.2",
    "lodash": "4.17.21"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
