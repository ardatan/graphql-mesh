{
  "name": "@graphql-mesh/json-schema",
  "version": "0.28.7",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/json-schema"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "json-machete": "0.8.3",
    "@graphql-mesh/types": "0.72.1",
    "@graphql-mesh/utils": "0.34.6",
    "@graphql-mesh/store": "0.8.4",
    "@graphql-tools/utils": "8.6.10",
    "graphql-compose": "9.0.8",
    "@omnigraph/json-schema": "0.19.4",
    "tslib": "^2.3.1",
    "@json-schema-tools/meta-schema": "1.6.19"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
