{
  "name": "@graphql-mesh/json-schema",
  "version": "0.22.4",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "json-machete": "0.0.25",
    "@graphql-mesh/types": "0.55.0",
    "@graphql-mesh/utils": "0.21.1",
    "@graphql-mesh/store": "0.2.3",
    "@graphql-tools/utils": "8.5.3",
    "graphql-compose": "9.0.4",
    "@omnigraph/json-schema": "0.3.3"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
