{
  "name": "@graphql-mesh/json-schema",
  "version": "0.25.4",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "json-machete": "0.5.14",
    "@graphql-mesh/types": "0.66.3",
    "@graphql-mesh/utils": "0.28.3",
    "@graphql-mesh/store": "0.3.26",
    "@graphql-tools/utils": "8.6.2",
    "graphql-compose": "9.0.7",
    "@omnigraph/json-schema": "0.17.4",
    "tslib": "^2.3.1",
    "@json-schema-tools/meta-schema": "1.6.19"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
