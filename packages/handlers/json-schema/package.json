{
  "name": "@graphql-mesh/json-schema",
  "version": "0.35.38",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/json-schema"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.10",
    "@graphql-mesh/store": "0.8.65",
    "@graphql-mesh/string-interpolation": "0.3.3",
    "@graphql-mesh/types": "0.87.1",
    "@graphql-mesh/utils": "0.42.9",
    "@graphql-tools/utils": "9.1.3",
    "@json-schema-tools/meta-schema": "1.7.0",
    "@omnigraph/json-schema": "0.36.11",
    "graphql-compose": "9.0.10",
    "json-machete": "0.16.5",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
