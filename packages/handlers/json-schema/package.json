{
  "name": "@graphql-mesh/json-schema",
  "version": "0.19.4",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/to-json-schema": "0.2.1",
    "@types/url-join": "4.0.1",
    "@types/qs": "6.9.7"
  },
  "dependencies": {
    "json-machete": "0.0.15",
    "@graphql-mesh/types": "0.49.0",
    "@graphql-mesh/utils": "0.17.0",
    "@graphql-mesh/store": "0.1.14",
    "@graphql-tools/utils": "8.2.4",
    "graphql-compose": "9.0.3",
    "graphql-scalars": "1.11.0",
    "pascal-case": "3.1.2",
    "to-json-schema": "0.2.5",
    "url-join": "4.0.1",
    "qs": "6.10.1",
    "ajv": "8.6.3",
    "ajv-formats": "2.1.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
