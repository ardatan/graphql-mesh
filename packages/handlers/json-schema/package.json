{
  "name": "@graphql-mesh/json-schema",
  "version": "0.24.4",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "json-machete": "0.5.0",
    "@graphql-mesh/types": "0.61.0",
    "@graphql-mesh/utils": "0.27.0",
    "@graphql-mesh/store": "0.3.13",
    "@graphql-tools/utils": "8.6.1",
    "graphql-compose": "9.0.6",
    "@omnigraph/json-schema": "0.13.3"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
