{
  "name": "@graphql-mesh/json-schema",
  "version": "0.97.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/json-schema"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.1",
    "@graphql-mesh/store": "^0.96.0",
    "@graphql-mesh/types": "^0.96.0",
    "@graphql-mesh/utils": "^0.96.0",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.5.3",
    "@json-schema-tools/meta-schema": "1.7.0",
    "@omnigraph/json-schema": "0.97.0",
    "graphql-compose": "9.0.10",
    "json-machete": "0.97.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
