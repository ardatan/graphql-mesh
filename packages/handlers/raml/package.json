{
  "name": "@graphql-mesh/raml",
  "version": "0.1.15",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.3.20",
    "@graphql-mesh/types": "0.64.1",
    "@omnigraph/raml": "0.5.3",
    "@graphql-mesh/utils": "0.27.7"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "buildOptions": {
    "external": [
      "cross-fetch"
    ]
  }
}
