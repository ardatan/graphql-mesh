{
  "name": "@graphql-mesh/thrift",
  "version": "0.8.41",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/thrift": "0.10.11",
    "@types/url-join": "4.0.1"
  },
  "dependencies": {
    "@creditkarma/thrift-client": "0.17.2",
    "@creditkarma/thrift-parser": "1.2.0",
    "@creditkarma/thrift-server-core": "0.17.2",
    "@graphql-mesh/types": "0.70.0",
    "@graphql-mesh/store": "0.7.0",
    "@graphql-mesh/utils": "0.33.0",
    "@graphql-tools/utils": "8.6.5",
    "fetchache": "0.1.1",
    "graphql-scalars": "1.17.0",
    "url-join": "4.0.1",
    "thrift": "0.16.0",
    "tslib": "^2.3.1",
    "pascal-case": "3.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
