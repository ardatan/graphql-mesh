{
  "name": "@graphql-mesh/thrift",
  "version": "0.12.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/thrift"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@creditkarma/thrift-client": "1.0.4",
    "@creditkarma/thrift-parser": "2.0.0",
    "@creditkarma/thrift-server-core": "1.0.4",
    "@graphql-mesh/cross-helpers": "0.3.0",
    "@graphql-mesh/store": "0.9.3",
    "@graphql-mesh/string-interpolation": "0.4.1",
    "@graphql-mesh/types": "0.89.2",
    "@graphql-mesh/utils": "0.43.3",
    "@graphql-tools/utils": "9.1.3",
    "graphql-scalars": "1.20.1",
    "pascal-case": "3.1.2",
    "thrift": "0.17.0",
    "tslib": "^2.4.0",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@types/thrift": "0.10.11",
    "@types/url-join": "4.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
