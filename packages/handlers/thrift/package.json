{
  "name": "@graphql-mesh/thrift",
  "version": "0.11.18",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/thrift"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/thrift": "0.10.11",
    "@types/url-join": "4.0.1"
  },
  "dependencies": {
    "@creditkarma/thrift-client": "1.0.4",
    "@creditkarma/thrift-parser": "2.0.0",
    "@creditkarma/thrift-server-core": "1.0.4",
    "@graphql-mesh/cross-helpers": "0.2.6",
    "@graphql-mesh/types": "0.84.6",
    "@graphql-mesh/store": "0.8.50",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "@graphql-mesh/utils": "0.41.17",
    "@graphql-tools/utils": "8.12.0",
    "graphql-scalars": "1.18.0",
    "url-join": "5.0.0",
    "thrift": "0.17.0",
    "tslib": "^2.4.0",
    "pascal-case": "3.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
