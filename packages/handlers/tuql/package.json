{
  "name": "@graphql-mesh/tuql",
  "version": "0.5.40",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/tuql"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.1",
    "@graphql-mesh/types": "0.78.7",
    "tuql": "1.7.0",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
