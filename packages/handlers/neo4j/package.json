{
  "name": "@graphql-mesh/neo4j",
  "version": "0.18.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/neo4j"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@neo4j/introspector": "^1.0.1",
    "@neo4j/graphql": "3.4.0",
    "@graphql-mesh/cross-helpers": "0.1.6",
    "@graphql-mesh/store": "0.8.17",
    "@graphql-mesh/utils": "0.36.0",
    "@graphql-mesh/types": "0.76.0",
    "neo4j-driver": "4.4.6",
    "tslib": "^2.4.0",
    "@graphql-tools/utils": "8.6.13"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
