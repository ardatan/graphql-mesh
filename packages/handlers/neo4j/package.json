{
  "name": "@graphql-mesh/neo4j",
  "version": "0.17.7",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/neo4j"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@neo4j/introspector": "^1.0.1",
    "@neo4j/graphql": "3.2.3",
    "@graphql-mesh/store": "0.8.7",
    "@graphql-mesh/utils": "0.34.9",
    "@graphql-mesh/types": "0.72.4",
    "neo4j-driver": "4.4.5",
    "tslib": "^2.3.1",
    "@graphql-tools/utils": "8.6.12"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
