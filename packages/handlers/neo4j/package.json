{
  "name": "@graphql-mesh/neo4j",
  "version": "0.20.34",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "buildOptions": {
    "input": "./src/index.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/neo4j"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@neo4j/introspector": "^1.0.1",
    "@neo4j/graphql": "3.12.0",
    "@graphql-mesh/cross-helpers": "0.2.9",
    "@graphql-mesh/store": "0.8.62",
    "@graphql-mesh/utils": "0.42.6",
    "@graphql-mesh/types": "0.85.7",
    "graphql-scalars": "1.20.1",
    "neo4j-driver": "5.2.0",
    "tslib": "^2.4.0",
    "@graphql-tools/utils": "9.1.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
