{
  "name": "@graphql-mesh/neo4j",
  "version": "0.20.38",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/handlers/neo4j"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.10",
    "@graphql-mesh/store": "0.8.64",
    "@graphql-mesh/types": "0.87.0",
    "@graphql-mesh/utils": "0.42.8",
    "@graphql-tools/utils": "9.1.1",
    "@neo4j/graphql": "3.12.2",
    "@neo4j/introspector": "^1.0.1",
    "graphql-scalars": "1.20.1",
    "neo4j-driver": "5.3.0",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
