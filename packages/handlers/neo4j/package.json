{
  "name": "@graphql-mesh/neo4j",
  "version": "0.12.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.1.0",
    "@graphql-mesh/utils": "0.13.0",
    "@graphql-mesh/types": "0.43.0",
    "neo4j-driver": "4.3.2",
    "neo4j-graphql-js": "2.19.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
