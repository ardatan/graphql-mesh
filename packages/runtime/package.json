{
  "name": "@graphql-mesh/runtime",
  "version": "0.46.22",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/runtime"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/types": "^0.91.13",
    "@graphql-mesh/utils": "^0.43.21",
    "@graphql-tools/utils": "^9.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@envelop/core": "3.0.6",
    "@envelop/extended-validation": "2.0.6",
    "@graphql-mesh/string-interpolation": "0.4.4",
    "@graphql-tools/batch-delegate": "8.4.22",
    "@graphql-tools/batch-execute": "8.5.18",
    "@graphql-tools/delegate": "9.0.28",
    "@graphql-tools/wrap": "9.3.8",
    "@whatwg-node/fetch": "^0.8.3"
  },
  "devDependencies": {
    "@graphql-tools/schema": "9.0.17"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
