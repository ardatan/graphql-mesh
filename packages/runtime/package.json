{
  "name": "@graphql-mesh/runtime",
  "version": "0.31.1",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@n1ru4l/graphql-live-query": "0.9.0",
    "@n1ru4l/in-memory-live-query-store": "0.9.0",
    "@graphql-mesh/types": "0.63.0",
    "@graphql-mesh/utils": "0.27.4",
    "@graphql-tools/delegate": "8.5.0",
    "@graphql-tools/batch-delegate": "8.2.3",
    "@graphql-tools/resolvers-composition": "6.4.1",
    "@graphql-tools/schema": "8.3.1",
    "@graphql-tools/wrap": "8.4.1",
    "@graphql-tools/utils": "8.6.1",
    "@envelop/core": "2.0.0",
    "@envelop/live-query": "3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
