{
  "name": "@graphql-mesh/runtime",
  "version": "0.44.25",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/runtime"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.6",
    "@graphql-mesh/types": "0.85.1",
    "@graphql-mesh/utils": "0.42.0",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "@graphql-tools/delegate": "9.0.8",
    "@graphql-tools/batch-delegate": "8.4.1",
    "@graphql-tools/batch-execute": "8.5.6",
    "@graphql-tools/wrap": "9.2.3",
    "@graphql-tools/utils": "8.12.0",
    "@whatwg-node/fetch": "^0.4.6",
    "@envelop/core": "3.0.2",
    "@envelop/extended-validation": "2.0.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-tools/schema": "9.0.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
