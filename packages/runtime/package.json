{
  "name": "@graphql-mesh/runtime",
  "version": "0.42.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/runtime"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-mesh/types": "0.79.0",
    "@graphql-mesh/utils": "0.38.0",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "@graphql-tools/delegate": "9.0.3",
    "@graphql-tools/batch-delegate": "8.3.6",
    "@graphql-tools/batch-execute": "8.5.3",
    "@graphql-tools/wrap": "9.0.4",
    "@graphql-tools/utils": "8.10.0",
    "@envelop/core": "2.5.0",
    "@envelop/extended-validation": "1.8.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-tools/schema": "9.0.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
