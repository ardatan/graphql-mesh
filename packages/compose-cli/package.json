{
  "name": "@graphql-mesh/compose-cli",
  "version": "0.7.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/compose-cli"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "mesh-compose": "dist/esm/bin.js"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/types": "^0.100.0",
    "graphql": "*"
  },
  "dependencies": {
    "@commander-js/extra-typings": "patch:@commander-js/extra-typings@npm%3A12.1.0#~/.yarn/patches/@commander-js-extra-typings-npm-12.1.0-65a781f8c6.patch",
    "@graphql-mesh/fusion-composition": "^0.2.0",
    "@graphql-mesh/include": "^0.0.1",
    "@graphql-mesh/utils": "^0.100.0",
    "@graphql-tools/graphql-file-loader": "8.0.1",
    "@graphql-tools/load": "^8.0.1",
    "@graphql-tools/schema": "^10.0.5",
    "@graphql-tools/utils": "^10.5.2",
    "@whatwg-node/fetch": "^0.9.14",
    "commander": "^12.0.0",
    "dotenv": "^16.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "bin": {
      "mesh-compose": {
        "input": "src/bin.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
