    "@apollo/gateway": "0.50.1",
    "@graphql-tools/schema": "8.3.8",
    "@graphql-mesh/utils": "0.33.3",
    "@graphql-tools/utils": "8.6.7",
    "@graphql-mesh/store": "0.7.3",
