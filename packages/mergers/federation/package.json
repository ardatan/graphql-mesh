      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/mergers/federation"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.80.2",
    "@graphql-tools/wrap": "9.0.5",
    "@apollo/gateway": "2.1.0",
    "@graphql-tools/schema": "9.0.2",
    "@graphql-mesh/cross-helpers": "0.2.2",
    "@graphql-mesh/utils": "0.40.0",
    "@graphql-tools/utils": "8.10.1",
    "@graphql-mesh/store": "0.8.32",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
