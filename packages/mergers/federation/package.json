{
  "name": "@graphql-mesh/merger-federation",
  "version": "0.11.21",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.63.0",
    "@graphql-tools/wrap": "8.4.0",
    "@apollo/gateway": "0.48.1",
    "@graphql-tools/schema": "8.3.1",
    "@graphql-mesh/utils": "0.27.4",
    "@graphql-tools/utils": "8.6.1",
    "@graphql-mesh/store": "0.3.17"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
