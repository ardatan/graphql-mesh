    "@graphql-tools/wrap": "8.4.4",
    "@graphql-mesh/utils": "0.28.4",
    "@graphql-tools/delegate": "8.5.3",
    "@graphql-tools/stitching-directives": "2.2.2",
    "@graphql-tools/schema": "8.3.2",
    "@graphql-mesh/store": "0.3.27",
    "@graphql-tools/utils": "8.6.2",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
