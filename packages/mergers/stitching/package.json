{
  "name": "@graphql-mesh/merger-stitching",
  "version": "0.18.12",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/mergers/stitching"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/store": "0.9.10",
    "@graphql-mesh/types": "0.91.2",
    "@graphql-mesh/utils": "0.43.10",
    "@graphql-tools/delegate": "9.0.27",
    "@graphql-tools/schema": "9.0.16",
    "@graphql-tools/stitch": "8.7.41",
    "@graphql-tools/stitching-directives": "2.3.30",
    "@graphql-tools/utils": "9.2.1",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
