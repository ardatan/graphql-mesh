{
  "name": "@omnigraph/json-schema",
  "version": "0.100.12",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/loaders/json-schema"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.3",
    "@graphql-mesh/types": "^0.98.8",
    "@graphql-mesh/utils": "^0.98.8",
    "@graphql-tools/utils": "^10.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.5.4",
    "@graphql-mesh/transport-common": "^0.2.8",
    "@graphql-mesh/transport-rest": "^0.2.8",
    "@graphql-tools/delegate": "^10.0.11",
    "@json-schema-tools/meta-schema": "1.7.5",
    "@whatwg-node/fetch": "^0.9.0",
    "ajv": "8.16.0",
    "ajv-formats": "2.1.1",
    "dset": "3.1.3",
    "graphql-compose": "9.0.11",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "json-machete": "0.97.3",
    "pascal-case": "3.1.2",
    "qs": "6.12.1",
    "to-json-schema": "0.2.5",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@types/qs": "6.9.15",
    "@types/to-json-schema": "0.2.4",
    "@types/url-join": "4.0.3"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
