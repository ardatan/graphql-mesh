{
  "name": "@omnigraph/json-schema",
  "version": "0.17.14",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/to-json-schema": "0.2.1",
    "@types/url-join": "4.0.1",
    "@types/qs": "6.9.7"
  },
  "dependencies": {
    "cross-undici-fetch": "0.1.27",
    "json-machete": "0.7.1",
    "@graphql-mesh/types": "0.68.3",
    "@graphql-mesh/utils": "0.32.1",
    "@graphql-tools/utils": "8.6.4",
    "to-json-schema": "0.2.5",
    "graphql-compose": "9.0.7",
    "graphql-scalars": "1.15.0",
    "ajv": "8.10.0",
    "ajv-formats": "2.1.1",
    "url-join": "4.0.1",
    "qs": "6.10.3",
    "pascal-case": "3.1.2",
    "lodash": "4.17.21",
    "tslib": "^2.3.1",
    "@json-schema-tools/meta-schema": "1.6.19"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
