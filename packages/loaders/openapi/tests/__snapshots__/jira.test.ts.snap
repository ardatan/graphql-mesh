            "name": "fields",
            "type": "array",
          },
          "fieldsByKeys": {
            "default": false,
            "description": "Whether fields in \`fields\` are referenced by keys rather than IDs. This parameter is useful where fields have been added by a connect app and a field's key may differ from its ID.",
            "name": "fieldsByKeys",
            "type": "boolean",
