{
  "name": "@omnigraph/openapi",
  "version": "0.0.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@omnigraph/json-schema": "0.7.1",
    "@graphql-mesh/utils": "0.24.1",
    "pluralize": "^8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
