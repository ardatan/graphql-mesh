{
  "name": "@omnigraph/openapi",
  "version": "0.19.27",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/openapi"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/types": "^0.91.13",
    "@graphql-mesh/utils": "^0.43.21",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.4.4",
    "@omnigraph/json-schema": "0.39.0",
    "change-case": "4.1.2",
    "json-machete": "0.18.20",
    "openapi-types": "12.1.0"
  },
  "devDependencies": {
    "@graphql-tools/utils": "9.2.1",
    "@whatwg-node/fetch": "0.8.4",
    "@whatwg-node/router": "0.3.0",
    "graphql-yoga": "3.8.0",
    "json-bigint-patch": "0.0.8"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
