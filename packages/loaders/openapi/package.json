      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.10",
    "@graphql-mesh/string-interpolation": "0.3.3",
    "@graphql-mesh/types": "0.87.1",
    "@graphql-mesh/utils": "0.42.9",
    "@omnigraph/json-schema": "0.36.11",
    "change-case": "4.1.2",
    "json-machete": "0.16.5",
    "openapi-types": "12.0.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-tools/utils": "9.1.1",
    "@types/cookie-parser": "1.4.3",
    "@types/multer": "1.4.7",
    "@whatwg-node/fetch": "0.5.3",
    "@whatwg-node/server": "0.4.17",
    "body-parser": "1.20.1",
    "cookie-parser": "1.4.6",
    "express": "4.18.2",
    "get-port": "6.1.2",
    "graphql-yoga": "3.1.0",
    "itty-router": "2.6.6",
    "itty-router-extras": "0.4.2",
    "json-bigint-patch": "0.0.8",
    "multer": "1.4.5-lts.1"
  },
  "publishConfig": {
