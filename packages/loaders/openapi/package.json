{
  "name": "@omnigraph/openapi",
  "version": "0.13.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/openapi"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.2",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "@graphql-mesh/types": "0.80.2",
    "@graphql-mesh/utils": "0.40.0",
    "@omnigraph/json-schema": "0.31.0",
    "change-case": "4.1.2",
    "json-machete": "0.13.2",
    "openapi-types": "12.0.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-tools/utils": "8.10.0",
    "@graphql-yoga/node": "2.13.9",
    "@types/cookie-parser": "1.4.3",
    "@types/multer": "1.4.7",
    "@whatwg-node/fetch": "0.3.2",
    "body-parser": "1.20.0",
    "cookie-parser": "1.4.6",
    "express": "4.18.1",
    "json-bigint-patch": "0.0.8",
    "multer": "1.4.5-lts.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
