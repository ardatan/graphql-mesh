{
  "name": "@omnigraph/openapi",
  "version": "0.4.7",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/openapi"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@omnigraph/json-schema": "0.17.24",
    "@graphql-mesh/utils": "0.33.4",
    "@graphql-mesh/types": "0.70.4",
    "json-machete": "0.7.10",
    "openapi-types": "11.0.0",
    "change-case": "4.1.2",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
