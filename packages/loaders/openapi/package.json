{
  "name": "@omnigraph/openapi",
  "version": "0.8.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/openapi"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.0",
    "@graphql-mesh/utils": "0.37.7",
    "@graphql-mesh/string-interpolation": "0.3.0",
    "@graphql-mesh/types": "0.78.6",
    "@omnigraph/json-schema": "0.26.0",
    "json-machete": "0.11.0",
    "openapi-types": "12.0.0",
    "change-case": "4.1.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@whatwg-node/fetch": "0.2.8",
    "@graphql-tools/utils": "8.9.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
