{
  "name": "@omnigraph/openapi",
  "version": "0.17.11",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/openapi"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
