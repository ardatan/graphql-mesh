{
  "name": "@omnigraph/raml",
  "version": "0.7.43",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/loaders/raml"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
