{
  "name": "@omnigraph/soap",
  "version": "0.107.21",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/loaders/soap"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "^0.4.11",
    "@graphql-mesh/string-interpolation": "^0.5.10",
    "@graphql-mesh/transport-common": "^1.0.13",
    "@graphql-mesh/transport-soap": "^0.10.21",
    "@graphql-mesh/types": "^0.104.18",
    "@graphql-mesh/utils": "^0.104.19",
    "@graphql-tools/utils": "^11.0.0",
    "@whatwg-node/fetch": "^0.10.6",
    "@whatwg-node/promise-helpers": "^1.0.0",
    "fast-xml-parser": "^5.3.3",
    "graphql-compose": "^9.0.11",
    "graphql-scalars": "^1.22.4"
  },
  "devDependencies": {
    "graphql": "16.12.0",
    "soap": "1.6.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
