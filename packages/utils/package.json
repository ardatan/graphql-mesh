{
  "name": "@graphql-mesh/utils",
  "version": "0.34.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/utils"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/is-url": "1.2.30",
    "@types/object-hash": "2.2.1",
    "@types/js-yaml": "4.0.5"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.1.0",
    "@graphql-tools/delegate": "8.7.8",
    "@graphql-tools/utils": "8.6.10",
    "@graphql-typed-document-node/core": "3.1.1",
    "@graphql-mesh/cross-helpers": "0.1.2",
    "@graphql-mesh/types": "0.72.1",
    "cross-undici-fetch": "^0.4.0",
    "chalk": "4.1.2",
    "dayjs": "1.11.2",
    "fetchache": "0.1.2",
    "flatstr": "1.0.12",
    "graphql-jit": "0.7.3",
    "graphql-subscriptions": "2.0.0",
    "lodash": "4.17.21",
    "is-url": "1.2.4",
    "js-yaml": "4.1.0",
    "object-hash": "3.0.0",
    "tiny-lru": "8.0.2",
    "tslib": "^2.3.1",
    "file-uri-to-path": "2.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
