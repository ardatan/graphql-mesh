    "@graphql-tools/utils": "^9.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@graphql-mesh/string-interpolation": "0.4.4",
    "@graphql-tools/delegate": "9.0.28",
    "dset": "3.1.2",
    "js-yaml": "4.1.0",
    "lodash.get": "4.4.2",
    "lodash.topath": "4.5.2",
    "tiny-lru": "8.0.2"
  },
  "devDependencies": {
    "@apollo/server": "4.6.0",
    "@types/js-yaml": "4.0.5",
    "@types/lodash.topath": "4.5.7",
    "@types/object-hash": "3.0.2",
    "@whatwg-node/fetch": "0.8.4"
