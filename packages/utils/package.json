{
  "name": "@graphql-mesh/utils",
  "version": "0.17.2",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/is-url": "1.2.30",
    "@types/object-hash": "2.2.1"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@ardatan/string-interpolation": "1.2.12",
    "@graphql-tools/delegate": "8.2.2",
    "@graphql-tools/utils": "8.2.5",
    "@graphql-typed-document-node/core": "3.1.0",
    "@graphql-mesh/types": "0.51.0",
    "cross-fetch": "3.1.4",
    "chalk": "4.1.2",
    "date-fns": "2.25.0",
    "fetchache": "0.1.1",
    "flatstr": "1.0.12",
    "graphql-jit": "0.6.0",
    "graphql-subscriptions": "1.2.1",
    "lodash": "4.17.21",
    "is-url": "1.2.4",
    "js-yaml": "4.1.0",
    "object-hash": "2.2.0",
    "tiny-lru": "7.0.6"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
