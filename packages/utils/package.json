{
  "name": "@graphql-mesh/utils",
  "version": "0.30.0",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/is-url": "1.2.30",
    "@types/object-hash": "2.2.1",
    "@types/js-yaml": "4.0.5"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@ardatan/string-interpolation": "1.2.12",
    "@graphql-tools/delegate": "8.5.4",
    "@graphql-tools/utils": "8.6.3",
    "@graphql-typed-document-node/core": "3.1.1",
    "@graphql-mesh/types": "0.67.0",
    "cross-undici-fetch": "0.1.25",
    "chalk": "4.1.2",
    "date-fns": "2.28.0",
    "fetchache": "0.1.1",
    "flatstr": "1.0.12",
    "graphql-jit": "0.7.3",
    "graphql-subscriptions": "2.0.0",
    "lodash": "4.17.21",
    "is-url": "1.2.4",
    "js-yaml": "4.1.0",
    "object-hash": "3.0.0",
    "tiny-lru": "8.0.2",
    "tslib": "^2.3.1",
    "file-uri-to-path": "2.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
