{
  "name": "@graphql-mesh/apollo-link",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@apollo/client": "3.5.9"
  },
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.31.8",
    "graphql": "^15.2.0 || ^16.0.0",
    "@apollo/client": "^3.5.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
