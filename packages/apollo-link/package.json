{
  "name": "@graphql-mesh/apollo-link",
  "version": "3.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/apollo-link"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-tools/utils": "8.8.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@apollo/client": "3.6.9"
  },
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.39.0",
    "graphql": "^15.2.0 || ^16.0.0",
    "@apollo/client": "^3.5.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
