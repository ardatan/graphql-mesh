{
  "name": "@graphql-mesh/types",
  "version": "0.87.1",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/types"
  },
  "license": "MIT",
  "main": "dist/index.js",
