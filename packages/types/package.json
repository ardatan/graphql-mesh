{
  "name": "@graphql-mesh/types",
  "version": "0.42.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "fetchache": "0.1.1",
    "@graphql-tools/delegate": "beta",
    "@graphql-tools/utils": "beta",
    "@graphql-typed-document-node/core": "3.1.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
