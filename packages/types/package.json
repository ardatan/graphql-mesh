    "@graphql-tools/utils": "9.1.1",
    "@graphql-typed-document-node/core": "3.1.1",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/index.d.ts"
