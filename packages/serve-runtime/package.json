{
  "name": "@graphql-mesh/serve-runtime",
  "version": "0.5.11",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "ardatan/graphql-mesh",
    "directory": "packages/serve-runtime"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "generate-landing-page-html": "node scripts/generate-landing-page-html.js"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@envelop/core": "^5.0.0",
    "@graphql-hive/apollo": "^0.34.0",
    "@graphql-mesh/cross-helpers": "^0.4.4",
    "@graphql-mesh/fusion-runtime": "^0.5.10",
    "@graphql-mesh/plugin-hive": "^0.99.7",
    "@graphql-mesh/transport-common": "^0.4.7",
    "@graphql-mesh/transport-http": "^0.3.7",
    "@graphql-mesh/utils": "^0.99.7",
    "@graphql-tools/batch-delegate": "^9.0.3",
    "@graphql-tools/delegate": "^10.0.17",
    "@graphql-tools/executor-http": "^1.1.5",
    "@graphql-tools/executor-yoga": "^3.0.0",
    "@graphql-tools/federation": "^2.2.4",
    "@graphql-tools/stitch": "^9.2.10",
    "@graphql-tools/utils": "^10.2.3",
    "@graphql-tools/wrap": "^10.0.5",
    "@whatwg-node/disposablestack": "^0.0.1",
    "@whatwg-node/server": "^0.9.46",
    "graphql-yoga": "^5.6.0"
  },
  "devDependencies": {
    "@envelop/disable-introspection": "6.0.0",
    "graphql-sse": "^2.5.3",
    "html-minifier-terser": "7.2.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "./package.json": "./package.json"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
