{
  "name": "@graphql-mesh/transform-mock",
  "version": "0.12.4",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/faker": "5.5.9"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.57.0",
    "@graphql-mesh/utils": "0.24.0",
    "@graphql-tools/utils": "8.5.4",
    "@graphql-tools/mock": "8.4.4",
    "faker": "5.5.3",
    "graphql-scalars": "1.14.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
