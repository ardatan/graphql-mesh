    "graphql": "*"
  },
  "devDependencies": {
    "@types/faker": "5.5.9"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.70.5",
    "@graphql-mesh/utils": "0.33.5",
    "@graphql-tools/utils": "8.6.9",
    "@graphql-tools/mock": "8.6.8",
    "faker": "5.5.3",
    "graphql-scalars": "1.17.0",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
