{
  "name": "@graphql-mesh/transform-extend",
  "version": "0.6.16",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.43.0",
    "@graphql-mesh/utils": "0.13.0",
    "@graphql-tools/load": "7.1.0",
    "@graphql-tools/code-file-loader": "7.0.1",
    "@graphql-tools/graphql-file-loader": "7.0.1",
    "@graphql-tools/merge": "6.2.16"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
