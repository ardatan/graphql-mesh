{
  "name": "@graphql-mesh/transform-extend",
  "version": "0.10.75",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/extend"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.86.0",
    "@graphql-mesh/utils": "0.42.7",
    "@graphql-tools/load": "7.8.6",
    "@graphql-tools/code-file-loader": "7.3.13",
    "@graphql-tools/graphql-file-loader": "7.5.11",
    "@graphql-tools/schema": "9.0.10",
    "@graphql-tools/utils": "9.1.1",
    "@graphql-mesh/cross-helpers": "0.2.10",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
