{
  "name": "@graphql-mesh/transform-extend",
  "version": "0.10.18",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/extend"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.72.3",
    "@graphql-mesh/utils": "0.34.8",
    "@graphql-tools/load": "7.5.11",
    "@graphql-tools/code-file-loader": "7.2.15",
    "@graphql-tools/graphql-file-loader": "7.3.12",
    "@graphql-tools/schema": "8.3.11",
    "@graphql-tools/utils": "8.6.10",
    "@graphql-mesh/cross-helpers": "0.1.3",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
