{
  "name": "@graphql-mesh/transform-hoist-field",
  "version": "0.2.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/hoist-field"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
