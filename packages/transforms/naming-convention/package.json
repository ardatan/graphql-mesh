{
  "name": "@graphql-mesh/transform-naming-convention",
  "version": "0.11.18",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/naming-convention"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@graphql-mesh/cache-localforage": "^0.6.59",
    "@graphql-tools/schema": "9.0.8"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.85.6",
    "@graphql-mesh/utils": "0.42.5",
    "@graphql-tools/wrap": "9.2.9",
    "@graphql-tools/delegate": "9.0.14",
    "@graphql-tools/utils": "8.13.1",
    "change-case": "4.1.2",
    "graphql-scalars": "1.20.0",
    "lower-case": "2.0.2",
    "tslib": "2.4.1",
    "upper-case": "2.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
