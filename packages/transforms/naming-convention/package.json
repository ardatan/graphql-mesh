{
  "name": "@graphql-mesh/transform-naming-convention",
  "version": "0.8.7",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.57.1",
    "@graphql-mesh/utils": "0.25.0",
    "@graphql-tools/wrap": "8.3.2",
    "@graphql-tools/delegate": "8.4.3",
    "@graphql-tools/utils": "8.6.0",
    "change-case": "4.1.2",
    "upper-case": "2.0.2",
    "lower-case": "2.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
