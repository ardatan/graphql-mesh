{
  "name": "@graphql-mesh/transform-naming-convention",
  "version": "0.93.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/naming-convention"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/types": "^0.93.0",
    "@graphql-mesh/utils": "^0.93.0",
    "@graphql-tools/utils": "^9.2.1",
    "graphql": "*",
    "tslib": "^2.5.0"
  },
  "dependencies": {
    "@graphql-tools/delegate": "^9.0.32",
    "@graphql-tools/wrap": "^9.4.2",
    "change-case": "^4.1.2",
    "graphql-scalars": "^1.20.4",
    "lower-case": "^2.0.2",
    "upper-case": "^2.0.2"
  },
  "devDependencies": {
    "@graphql-mesh/cache-localforage": "^0.93.0",
    "@graphql-tools/schema": "9.0.19"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
