{
  "name": "@graphql-mesh/transform-replace-field",
  "version": "0.3.40",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/replace-field"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.70.3",
    "@graphql-mesh/utils": "0.33.3",
    "@graphql-tools/code-file-loader": "7.2.12",
    "@graphql-tools/graphql-file-loader": "7.3.9",
    "@graphql-tools/load": "7.5.8",
    "@graphql-tools/utils": "8.6.8",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "devDependencies": {
    "@graphql-mesh/cache-inmemory-lru": "^0.6.10",
    "@graphql-tools/schema": "8.3.8"
  }
}
