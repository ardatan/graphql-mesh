{
  "name": "@graphql-mesh/transform-replace-field",
  "version": "0.4.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/replace-field"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.88.0",
    "@graphql-mesh/utils": "0.43.0",
    "@graphql-tools/code-file-loader": "7.3.15",
    "@graphql-tools/graphql-file-loader": "7.5.13",
    "@graphql-tools/load": "7.8.8",
    "@graphql-tools/utils": "9.1.3",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-mesh/cache-localforage": "0.7.0",
    "@graphql-tools/schema": "9.0.12"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
