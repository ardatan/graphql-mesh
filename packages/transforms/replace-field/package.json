{
  "name": "@graphql-mesh/transform-replace-field",
  "version": "0.0.2",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.51.0",
    "@graphql-mesh/utils": "0.17.2",
    "@graphql-tools/code-file-loader": "7.2.1",
    "@graphql-tools/graphql-file-loader": "7.3.1",
    "@graphql-tools/load": "7.3.2",
    "@graphql-tools/utils": "8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "devDependencies": {
    "@graphql-mesh/cache-inmemory-lru": "^0.5.22",
    "@graphql-tools/schema": "8.2.0",
    "graphql-subscriptions": "1.2.1"
  }
}
