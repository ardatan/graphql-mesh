{
  "name": "@graphql-mesh/transform-cache",
  "version": "0.11.39",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/cache"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.70.1",
    "@graphql-mesh/utils": "0.33.1",
    "date-fns": "2.28.0",
    "@graphql-tools/resolvers-composition": "6.4.5",
    "@graphql-tools/schema": "8.3.6",
    "@graphql-tools/utils": "8.6.5",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
