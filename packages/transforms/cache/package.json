{
  "name": "@graphql-mesh/transform-cache",
  "version": "0.13.14",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/cache"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.3.3",
    "@graphql-mesh/string-interpolation": "0.4.2",
    "@graphql-mesh/types": "0.91.6",
    "@graphql-mesh/utils": "0.43.14",
    "@graphql-tools/resolvers-composition": "6.5.17",
    "@graphql-tools/schema": "9.0.16",
    "@graphql-tools/utils": "9.2.1",
    "dayjs": "1.11.7",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-mesh/cache-localforage": "0.7.14"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
