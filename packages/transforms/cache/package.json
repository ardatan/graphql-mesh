{
  "name": "@graphql-mesh/transform-cache",
  "version": "0.12.39",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/cache"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.82.3",
    "@graphql-mesh/utils": "0.41.4",
    "@graphql-mesh/string-interpolation": "0.3.2",
    "dayjs": "1.11.5",
    "@graphql-mesh/cross-helpers": "0.2.4",
    "@graphql-tools/resolvers-composition": "6.5.5",
    "@graphql-tools/schema": "9.0.3",
    "@graphql-tools/utils": "8.11.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-mesh/cache-localforage": "0.6.34"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
