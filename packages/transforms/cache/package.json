{
  "name": "@graphql-mesh/transform-cache",
  "version": "0.12.7",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/cache"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/types": "0.72.2",
    "@graphql-mesh/utils": "0.34.7",
    "dayjs": "1.11.2",
    "@graphql-tools/resolvers-composition": "6.4.10",
    "@graphql-tools/schema": "8.3.11",
    "@graphql-tools/utils": "8.6.10",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@graphql-mesh/cache-localforage": "0.6.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
