{
  "name": "@graphql-mesh/transform-federation",
  "version": "0.10.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/federation"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@apollo/subgraph": "2.2.1",
    "@graphql-mesh/string-interpolation": "0.4.0",
    "@graphql-mesh/types": "0.89.0",
    "@graphql-mesh/utils": "0.43.1",
    "@graphql-tools/delegate": "9.0.19",
    "@graphql-tools/stitching-directives": "2.3.22",
    "@graphql-tools/utils": "9.1.3",
    "graphql-transform-federation": "2.2.0",
    "lodash.set": "4.3.2",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
