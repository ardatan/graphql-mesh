    "url": "Urigo/graphql-mesh",
    "directory": "packages/transforms/federation"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@apollo/subgraph": "2.0.5",
    "@graphql-mesh/types": "0.73.3",
    "@graphql-mesh/utils": "0.35.3",
    "graphql-transform-federation": "2.2.0",
    "@graphql-tools/utils": "8.6.13",
    "@graphql-tools/stitching-directives": "2.2.18",
    "@graphql-tools/delegate": "8.7.11",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
