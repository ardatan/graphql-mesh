    "directory": "packages/transforms/federation"
  },
  "peerDependencies": {
    "graphql": "*",
    "@graphql-mesh/types": "^0.78.5"
  },
  "dependencies": {
    "@apollo/subgraph": "2.0.5",
    "@graphql-mesh/utils": "0.37.6",
    "graphql-transform-federation": "2.2.0",
    "@graphql-tools/utils": "8.9.0",
    "@graphql-tools/stitching-directives": "2.3.1",
    "@graphql-tools/delegate": "8.8.1",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
