{
  "name": "@graphql-mesh/config",
  "version": "8.0.31",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/config"
  },
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.44.31",
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.9",
    "@graphql-mesh/cache-localforage": "0.6.60",
    "@graphql-mesh/merger-bare": "0.15.46",
    "@graphql-mesh/merger-stitching": "0.17.6",
    "@graphql-mesh/types": "0.85.7",
    "@graphql-mesh/store": "0.8.62",
    "@graphql-mesh/utils": "0.42.6",
    "@graphql-tools/utils": "9.1.0",
    "@graphql-tools/load": "7.8.5",
    "@graphql-tools/code-file-loader": "7.3.11",
    "@graphql-tools/graphql-file-loader": "7.5.10",
    "@whatwg-node/fetch": "^0.5.0",
    "param-case": "3.0.4",
    "@envelop/core": "3.0.3",
    "tslib": "^2.4.0",
    "pascal-case": "3.1.2",
    "camel-case": "4.1.2"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
