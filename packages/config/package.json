{
  "name": "@graphql-mesh/config",
  "version": "10.1.15",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/config"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/cross-helpers": "^0.3.4",
    "@graphql-mesh/runtime": "^0.46.23",
    "@graphql-mesh/store": "^0.9.20",
    "@graphql-mesh/types": "^0.91.14",
    "@graphql-mesh/utils": "^0.43.22",
    "@graphql-tools/utils": "^9.2.1",
    "graphql": "*",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@envelop/core": "3.0.6",
    "@graphql-mesh/cache-localforage": "0.7.20",
    "@graphql-mesh/merger-bare": "0.16.24",
    "@graphql-mesh/merger-stitching": "0.18.24",
    "@graphql-tools/code-file-loader": "7.3.22",
    "@graphql-tools/graphql-file-loader": "7.5.17",
    "@graphql-tools/load": "7.8.14",
    "@whatwg-node/fetch": "^0.8.3",
    "camel-case": "4.1.2",
    "param-case": "3.0.4",
    "pascal-case": "3.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
