{
  "name": "@graphql-mesh/config",
  "version": "8.0.31",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/config"
  },
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.44.31",
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.9",
    "@graphql-mesh/cache-localforage": "0.6.60",
    "@graphql-mesh/merger-bare": "0.15.46",
    "@graphql-mesh/merger-stitching": "0.17.6",
    "@graphql-mesh/types": "0.85.7",
    "@graphql-mesh/store": "0.8.62",
    "@graphql-mesh/utils": "0.42.6",
    "@graphql-tools/utils": "9.1.1",
    "@graphql-tools/load": "7.8.6",
    "@graphql-tools/code-file-loader": "7.3.12",
    "@graphql-tools/graphql-file-loader": "7.5.11",
    "@whatwg-node/fetch": "^0.5.0",
    "param-case": "3.0.4",
    "@envelop/core": "3.0.3",
    "tslib": "^2.4.0",
    "pascal-case": "3.1.2",
    "camel-case": "4.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
