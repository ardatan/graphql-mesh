{
  "name": "@graphql-mesh/config",
  "version": "8.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/config"
  },
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.44.2",
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@graphql-mesh/cross-helpers": "0.2.4",
    "@graphql-mesh/cache-localforage": "0.6.37",
    "@graphql-mesh/merger-bare": "0.15.21",
    "@graphql-mesh/merger-stitching": "0.16.20",
    "@graphql-mesh/types": "0.83.2",
    "@graphql-mesh/store": "0.8.40",
    "@graphql-mesh/utils": "0.41.7",
    "@graphql-tools/utils": "8.12.0",
    "@graphql-tools/load": "7.7.7",
    "@graphql-tools/code-file-loader": "7.3.6",
    "@graphql-tools/graphql-file-loader": "7.5.5",
    "@whatwg-node/fetch": "0.4.2",
    "param-case": "3.0.4",
    "@envelop/core": "2.6.0",
    "tslib": "^2.4.0",
    "pascal-case": "3.1.2",
    "camel-case": "4.1.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
