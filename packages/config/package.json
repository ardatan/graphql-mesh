{
  "name": "@graphql-mesh/config",
  "version": "8.0.36",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/config"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@graphql-mesh/runtime": "^0.44.37",
    "graphql": "*"
  },
  "dependencies": {
    "@envelop/core": "3.0.4",
    "@graphql-mesh/cache-localforage": "0.6.63",
    "@graphql-mesh/cross-helpers": "0.2.10",
    "@graphql-mesh/merger-bare": "0.15.51",
    "@graphql-mesh/merger-stitching": "0.17.11",
    "@graphql-mesh/store": "0.8.65",
    "@graphql-mesh/types": "0.87.1",
    "@graphql-mesh/utils": "0.42.9",
    "@graphql-tools/code-file-loader": "7.3.13",
    "@graphql-tools/graphql-file-loader": "7.5.11",
    "@graphql-tools/load": "7.8.6",
    "@graphql-tools/utils": "9.1.1",
    "@whatwg-node/fetch": "^0.5.0",
    "camel-case": "4.1.2",
    "param-case": "3.0.4",
    "pascal-case": "3.1.2",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
