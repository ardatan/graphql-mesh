{
  "name": "@graphql-mesh/store",
  "version": "0.6.1",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "graphql": "16.3.0"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@graphql-inspector/core": "3.1.1",
    "@graphql-mesh/cross-helpers": "0.1.0",
    "@graphql-mesh/utils": "0.32.1",
    "@graphql-tools/utils": "8.6.5",
    "@graphql-mesh/types": "0.68.3",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
