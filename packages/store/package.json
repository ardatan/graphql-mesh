{
  "name": "@graphql-mesh/store",
  "version": "0.2.1",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "graphql": "16.0.1"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@graphql-inspector/core": "3.0.2",
    "@graphql-mesh/utils": "0.20.1",
    "@graphql-tools/utils": "8.5.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
