{
  "name": "@graphql-mesh/store",
  "version": "0.8.43",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/store"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "graphql": "16.6.0"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@graphql-inspector/core": "3.4.0",
    "@graphql-mesh/cross-helpers": "0.2.6",
    "@graphql-mesh/utils": "0.41.10",
    "@graphql-tools/utils": "8.12.0",
    "@graphql-mesh/types": "0.83.5",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
